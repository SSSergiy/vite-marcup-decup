title: Мой блог
permalink: /blog/
---

<!DOCTYPE html>
<html lang="ru">

<head>
	<meta charset="UTF-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<title>
		<%= title %>
	</title>
	<link rel="stylesheet" href="/style.css">
</head>

<body>
	<h1>
		<%= title %>
	</h1>

	<ul>
		<% for(let post of blog.posts) { %>
			<li>
				<h2>
					<%= post.title %>
				</h2>
				<p>
					<%= post.body %>
				</p>
			</li>
			<% } %>
	</ul>

	<script type="module" src="/main.js"></script>
</body>

</html>